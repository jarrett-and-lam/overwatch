{
    "name": "jarrett-and-lam/overwatch",
    "description": "Overwatch for Laravel",
    "license": "MIT",
    "authors": [
      {
        "name": "Joe Towner",
        "email": "joe.towner@jarrettandlam.com"
      }
    ],
    "require": {
      "php": ">=7.1",
      "illuminate/support": "5.8.*|^6.0"
    },
    "autoload": {
      "psr-4": {
        "JarrettAndLam\\Overwatch\\": "src/Overwatch/"
      }
    },
    "config": {
      "preferred-install": "dist"
    },
    "extra": {
      "laravel": {
        "providers": [
          "JarrettAndLam\\Overwatch\\Providers\\OverwatchServiceProvider"
        ]
      }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
  }
